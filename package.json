{
  "name": "sciris-uikit",
  "version": "0.1.0",
  "author": "Optima Consortium <info@ocds.co>",
  "scripts": {
    "build": "bili"
  },
  "main": "dist/sciris-uikit.cjs.js",
  "module": "dist/sciris-uikit.es.js",
  "files": [
    "dist/*",
    "src/*",
    "*.js",
    "*.json"
  ],
  "babel": {
    "presets": [
      [
        "vue-app"
      ]
    ]
  },
  "bili": {
    "css": false,
    "exports": "named",
    "plugins": [
      "vue",
      "copy-glob"
    ],
    "vue": {
      "css": false
    },
    "postcss": {
      "extract": false
    },
    "copy-glob": [
      {
        "files": "src/styles/*",
        "dest": "dist/scss"
      }
    ],
    "format": [
      "cjs",
      "es",
      "umd",
      "umd-min"
    ],
    "banner": true,
    "name": "sciris-uikit",
    "moduleName": "sciris-uikit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-preval": "^3.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-vue-app": "^2.0.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "normalize-scss": "^7.0.1",
    "popper.js": "^1.14.4",
    "sciris-js": "git+https://github.com/sciris/sciris-js.git",
    "vue": "^2.5.17",
    "vue-paper-dashboard": "git+https://github.com/creativetimofficial/vue-paper-dashboard.git",
    "vue2-simplert-plugin": "^0.5.3"
  },
  "devDependencies": {
    "bili": "^3.1.2",
    "node-sass": "^4.9.4",
    "parse5": "^2.2.3",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-copy-glob": "^0.2.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-vue": "^2.5.4"
  }
}
